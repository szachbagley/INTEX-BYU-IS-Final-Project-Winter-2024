@{
}

@if (ViewBag.recProducts != null)
{
    <h2>Recommended Products</h2>
    <ul>
        @foreach (var product in ViewBag.recProducts)
        {
            <li>@product.productName</li> <!-- Assuming Product has a Name property -->
        }
    </ul>
}
